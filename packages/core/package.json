{
  "name": "@shak-hooks/core",
  "version": "0.0.1",
  "description": "Framework-agnostic core logic for Shak Hooks",
  "homepage": "https://www.shaktools.com/shak-hooks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.ts"
  },
  "devDependencies": {
    "@shak-hooks/config-typescript": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
