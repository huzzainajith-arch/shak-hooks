{
  "name": "@shak-hooks/usehooks-angular",
  "version": "0.0.2",
  "description": "Angular signals utilities for Shak Hooks (convenience alias for @shak-hooks/angular)",
  "homepage": "https://www.shaktools.com/shak-hooks",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "@angular/core": ">=16.0.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "@shak-hooks/angular": "^0.0.2"
  },
  "devDependencies": {
    "@shak-hooks/config-typescript": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
