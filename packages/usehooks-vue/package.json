{
  "name": "@shak-hooks/usehooks-vue",
  "version": "0.0.1",
  "description": "Vue composables for Shak Hooks (convenience alias for @shak-hooks/vue)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@shak-hooks/vue": "^0.0.1"
  },
  "devDependencies": {
    "@shak-hooks/config-typescript": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
