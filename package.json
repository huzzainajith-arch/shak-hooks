{
  "name": "shak-hooks-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "turbo run build --filter=@shak-hooks/* && changeset publish",
    "test": "vitest run",
    "test:angular": "vitest run --config packages/angular/vitest.config.ts",
    "test:react": "vitest run packages/react/src",
    "test:vue": "vitest run packages/vue/src"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^4.0.16",
    "happy-dom": "^20.0.11",
    "prettier": "^3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tsup": "^7.2.0",
    "turbo": "^1.10.13",
    "typescript": "^5.2.2",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@8.7.1",
  "engines": {
    "node": ">=18"
  }
}
